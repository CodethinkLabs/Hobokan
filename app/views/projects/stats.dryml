<show-page>
<content:>
  <a action="kanban">
    &laquo; Back to <name/> kanban
  </a><br/>
  <h1>Tasks Created</h1>
  <%= @started %> <br/>
  <h1>Tasks Finished</h1>
  <%= @finished %>
  <section-group>
    <section>
      <table-plus with="&@todo" fields="title, lane, created_at"/>

    </section>
    <aside style="width:50%;">
      <change-log-view/>
    </aside>
  </section-group>
</content:>
</show-page>